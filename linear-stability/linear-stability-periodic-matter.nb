Cell[BoxData[
 RowBox[{
  RowBox[{"sol2BeamsNF", "[", 
   RowBox[{
   "eta_", ",", "mu_", ",", "alpha_", ",", "lambda0_", ",", "lambda1_", ",", 
    "km_", ",", "z_", ",", 
    RowBox[{"{", 
     RowBox[{"zrangestart_", ",", "zrangeend_"}], "}"}], ",", 
    RowBox[{"omegav_:", "1"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "delVecM", ",", "delMatM", ",", "delLM", ",", "delRM", ",", "sysM", ",", 
      "initM", ",", "zM", ",", "solM", ",", "thetaRM"}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"delVecM", "[", "zM_", "]"}], "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"delLM", "[", "zM", "]"}], ",", 
        RowBox[{"delRM", "[", "zM", "]"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"delMatM", "[", "zM_", "]"}], "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"lambda0", "+", 
           RowBox[{"lambda1", " ", 
            RowBox[{"Sin", "[", 
             RowBox[{"km", " ", "z"}], "]"}]}], "-", 
           RowBox[{"alpha", " ", 
            RowBox[{"mu", "/", "2"}]}], "-", 
           RowBox[{"eta", " ", "omegav"}]}], ",", 
          RowBox[{"alpha", " ", 
           RowBox[{"mu", " ", "/", "2"}]}]}], "\[IndentingNewLine]", "}"}], 
        ",", 
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "mu"}], "/", "2"}], ",", 
          RowBox[{"lambda0", "+", 
           RowBox[{"lambda1", " ", 
            RowBox[{"Sin", "[", 
             RowBox[{"km", " ", "z"}], "]"}]}], "+", 
           RowBox[{"mu", " ", "/", "2"}], "+", 
           RowBox[{"eta", " ", "omegav"}]}]}], "\[IndentingNewLine]", "}"}]}],
        "}"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"initM", "=", 
      RowBox[{
       RowBox[{"delVecM", "[", "0", "]"}], "==", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"10", "^", 
          RowBox[{"{", 
           RowBox[{"-", "6"}], "}"}]}], ",", 
         RowBox[{"10", "^", 
          RowBox[{"{", 
           RowBox[{"-", "6"}], "}"}]}]}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"sysM", "=", 
      RowBox[{
       RowBox[{"I", " ", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"delVecM", "[", "z", "]"}], ",", "z"}], "]"}]}], "\[Equal]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"delMatM", "[", "z", "]"}], ".", 
         RowBox[{"delVecM", "[", "z", "]"}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"solM", "=", 
      RowBox[{"NDSolve", "[", 
       RowBox[{
        RowBox[{"sysM", "&&", "initM"}], ",", 
        RowBox[{"{", 
         RowBox[{"delLM", ",", "delRM"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"z", ",", "zrangestart", ",", "zrangeend"}], "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"zM", ",", 
           RowBox[{"Log", "@", 
            RowBox[{"First", "@", 
             RowBox[{"Abs", "[", 
              RowBox[{
               RowBox[{"delLM", "[", "zM", "]"}], "/.", 
               RowBox[{"First", "@", "solM"}]}], "]"}]}]}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"zM", ",", "zrangestart", ",", "zrangeend", ",", "1"}], 
          "}"}]}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"zM", ",", 
           RowBox[{"Log", "@", 
            RowBox[{"First", "@", 
             RowBox[{"Abs", "[", 
              RowBox[{
               RowBox[{"delRM", "[", "zM", "]"}], "/.", 
               RowBox[{"First", "@", "solM"}]}], "]"}]}]}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"zM", ",", "zrangestart", ",", "zrangeend", ",", "1"}], 
          "}"}]}], "]"}]}], "\[IndentingNewLine]", "}"}]}]}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
