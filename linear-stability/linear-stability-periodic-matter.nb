Cell[BoxData[
 RowBox[{
  RowBox[{"theoryKappa2BeamsNF", "[", 
   RowBox[{"eta_", ",", "mu_", ",", "alpha_", ",", "lambda0_"}], "]"}], ":=", 
  
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "omegaM", ",", "alphaM", ",", "muM", ",", "etaM", ",", "thetaRM"}], 
     "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"etaM", "=", "eta"}], ";", "\[IndentingNewLine]", 
     RowBox[{"omegaM", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"muM", "=", "mu"}], ";", "\[IndentingNewLine]", 
     RowBox[{"alphaM", "=", "alpha"}], " ", ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\"\<Upper Limit \>\"", "<>", 
        RowBox[{"ToString", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"muM", "/", "omegaM"}], "]"}], "<", 
          RowBox[{"4", "/", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "-", 
              SqrtBox["alphaM"]}], ")"}], "^", "2"}]}]}], 
         "\[IndentingNewLine]", "]"}]}], ",", 
       RowBox[{"\"\<Lower Limit \>\"", "<>", 
        RowBox[{"ToString", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"4", "/", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "+", 
              SqrtBox["alphaM"]}], ")"}], "^", "2"}]}], "<", 
          RowBox[{"Abs", "[", 
           RowBox[{"muM", "/", "omegaM"}], "]"}]}], "\[IndentingNewLine]", 
         "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"N", "@", "omegaM"}], 
        RowBox[{
         SqrtBox[
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"1", "-", "alphaM"}], ")"}], "^", "2"}], 
            RowBox[{
             RowBox[{"muM", "^", "2"}], "/", 
             RowBox[{"omegaM", "^", "2"}]}]}], "-", 
           RowBox[{"8", "etaM", 
            RowBox[{"(", 
             RowBox[{"1", "+", "alphaM"}], ")"}], 
            RowBox[{"muM", "/", "omegaM"}]}]}]], "/", "4"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"N", "@", 
         RowBox[{"Im", "@", 
          SqrtBox[
           RowBox[{
            RowBox[{
             RowBox[{"-", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"1", "-", "alphaM"}], ")"}], "^", "2"}]}], 
             RowBox[{"muM", "^", "2"}]}], "+", 
            RowBox[{"8", "etaM", 
             RowBox[{"(", 
              RowBox[{"1", "+", "alphaM"}], ")"}], 
             RowBox[{"muM", "/", "omegaM"}]}]}]]}]}], "/", "4"}]}], "}"}]}]}],
    "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
   "]"}]}]], "Input",
